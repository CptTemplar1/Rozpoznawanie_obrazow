                                stop: 0.5 #333333, stop: 1.0 #222222); /* dostosuj gradient tła */
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #222222, stop: 0.4 #333333,
                                stop: 0.5 #444444, stop: 1.0 #555555); /* dostosuj gradient tła dla stanu &quot;on&quot; */
}

QComboBox:on {
    padding-top: 3px;
    padding-left: 4px;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left-width: 1px;
    border-left-color: #555555; /* zmień kolor obramowania strzałki */
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
